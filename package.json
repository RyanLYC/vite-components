{
  "name": "vite-components-last",
  "version": "0.0.29",
  "private": false,
  "type": "module",
  "license": "MIT",
  "main": "dist/index.es.js",
  "module": "dist/index.es.ts",
  "unpkg": "dist/index.umd.js",
  "types": "./dist/types/index.d.ts",
  "exports": {
    ".": {
      "types": "./dist/types/index.d.ts",
      "import": "./dist/index.es.js",
      "require": "./dist/index.umd.js"
    },
    "./icon": {
      "types": "./dist/types/icon/index.d.ts",
      "import": "./dist/icon/index.es.js",
      "require": "./dist/icon/index.umd.js"
    },
    "./*": "./*"
  },
  "files": [
    "dist",
    "types"
  ],
  "scripts": {
    "dev": "vite",
    "type-check": "vue-tsc --noEmit -p tsconfig.app.json --composite false",
    "lint": "eslint . --ext .vue,.js,.jsx,.cjs,.mjs,.ts,.tsx,.cts,.mts --fix --ignore-path .gitignore",
    "format": "prettier --config .prettierrc.cjs --write src/",
    "prepare": "husky install",
    "docs:dev": "vitepress dev docs",
    "docs:build": "vitepress build docs",
    "docs:serve": "vitepress serve docs",
    "gen": "node scripts/createComponent.mjs",
    "gen:icon": "node scripts/genIcons.mjs",
    "build:icon": "node scripts/build-icon.cjs",
    "build:type": "最新写法的vue文件解释有问题 node scripts/build-types.cjs",
    "build:tsc": "vue-tsc -p tsconfig.declaration.json",
    "build": "rimraf ./dist && vite build && yarn build:icon && yarn build:tsc && node scripts/genGlobalVueDts.cjs",
    "test": "vitest",
    "coverage": "vitest run --coverage"
  },
  "dependencies": {
    "lodash-es": "^4.17.21",
    "vite-components-last": "^0.0.29",
    "vue": "^3.3.4"
  },
  "config": {
    "commitizen": {
      "path": "./node_modules/cz-customizable"
    },
    "cz-customizable": {
      "config": ".cz-config.cjs"
    }
  },
  "lint-staged": {
    "*.{js,jsx,ts,tsx,vue}": [
      "yarn lint",
      "yarn format"
    ]
  },
  "devDependencies": {
    "@commitlint/cli": "^17.7.1",
    "@commitlint/config-conventional": "^17.7.0",
    "@rushstack/eslint-patch": "^1.3.3",
    "@tsconfig/node18": "^18.2.2",
    "@types/lodash-es": "^4.17.10",
    "@types/node": "^18.17.17",
    "@vitejs/plugin-vue": "^4.3.4",
    "@vitejs/plugin-vue-jsx": "^3.0.2",
    "@vitepress-demo-preview/component": "^2.3.2",
    "@vitepress-demo-preview/plugin": "^1.2.2",
    "@vue/compiler-sfc": "^3.3.4",
    "@vue/eslint-config-prettier": "^8.0.0",
    "@vue/eslint-config-typescript": "^12.0.0",
    "@vue/test-utils": "^2.4.1",
    "@vue/tsconfig": "^0.4.0",
    "commitizen": "^4.3.0",
    "cz-customizable": "^7.0.0",
    "eslint": "^8.49.0",
    "eslint-plugin-vue": "^9.17.0",
    "fast-glob": "^3.3.1",
    "fs": "^0.0.1-security",
    "fs-extra": "^11.1.1",
    "husky": "^8.0.3",
    "jsdom": "^22.1.0",
    "lint-staged": "^14.0.1",
    "npm-run-all2": "^6.0.6",
    "prettier": "^3.0.3",
    "sass": "^1.67.0",
    "svgo": "^3.0.2",
    "ts-morph": "^20.0.0",
    "typescript": "^5.2.2",
    "vite": "^4.4.11",
    "vite-plugin-eslint": "^1.8.1",
    "vitepress": "^1.0.0-rc.20",
    "vitest": "^0.34.6",
    "vue-tsc": "^1.8.18"
  }
}
